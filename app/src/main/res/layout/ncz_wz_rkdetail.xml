            android:layout_height="match_parent"
            android:layout_weight="1"
            android:gravity="center"
            android:inputType="numberDecimal"
            android:text="磷肥"
            android:textColor="@color/bg_text"
            android:textSize="@dimen/size_14"/>

    </LinearLayout>

    <View
